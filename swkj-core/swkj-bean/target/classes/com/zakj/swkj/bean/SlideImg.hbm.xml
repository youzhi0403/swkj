<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.zakj.swkj.bean.SlideImg" table="slide_img" >
        <id name="id" type="java.lang.Long">
            <column name="id"/>
            <generator class="identity" />
        </id>
        <property name="img_url" type="java.lang.String">
            <column name="img_url" />
        </property>
        <property name="deltag" type="java.lang.String">
            <column name="deltag" />
        </property>
        <property name="img_index" type="java.lang.String">
            <column name="img_index" />
        </property>
    </class>
</hibernate-mapping>
